{
  "name": "apparat/dev",
  "description": "Apparat development resources",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net"
    }
  ],
  "minimum-stability": "RC",
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "apparat/object": "@dev",
    "apparat/resource": "@dev",
    "apparat/kernel": "@dev",
    "dominicsayers/isemail": "@dev",
    "satooshi/php-coveralls": "^1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/object.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/resource.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dominicsayers/isemail.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    },
    "files": [
      "src/Dev/Module.php",
      "src/Dev/Autorun.php"
    ]
  }
}
